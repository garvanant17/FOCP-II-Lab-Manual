#include <iostream>
using namespace std;
bool isPrime(int a);

    int main(){
        int x;
    cout<<"enter an integer: ";
    cin>>x;
    isPrime(x);
    }
    bool isPrime(int a){
        int flag =0;
    for (int i=2;i<a-1; i++){
        if (a%i == 0)
        flag = flag +1;}
        if(flag == 0){
            cout<<"Prime";}
        else if(flag>0)
        cout<<"Not Prime";
}
